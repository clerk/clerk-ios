{
  "version": 1,
  "lineLength": 150,
  "indentation": {
    "spaces": 2
  },
  "lineBreakBeforeEachArgument": true,
  "prioritizeKeepingFunctionOutputTogether": true,
  "rules": {
    "AllPublicDeclarationsHaveDocumentation": true,
    "OrderedImports": true,
    "ValidateDocumentationComments": true
  }
}
